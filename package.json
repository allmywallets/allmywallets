    "lint:fix": "eslint --fix '@(app|test)/**/*.@(js|vue)'",
    "start": "webpack-dev-server --mode development --env.APP_VERSION=\"$(git describe --tags)\" --host 0.0.0.0 --devtool eval --progress --hot --content-base app --config config/webpack.dev.config.js",
    "build": "webpack --mode production --env.production --env.APP_VERSION=\"$(git describe --tags)\" --config config/webpack.prod.config.js",
    "test": "npm run test:unit && npm run test:functional",
    "test:unit": "jest test/",
    "test:functional": "concurrently 'http-server build/ -p 1337 -s' 'cypress run' --success first --kill-others"
  },
